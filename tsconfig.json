{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "preserve",
    "types": ["element-plus/global", "unplugin-icons/types/vue"],
    "paths": {
      "@/*": ["src/*"]
    },
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable",
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitThis": true,
    "strict": true,
    "isolatedModules": true,
  },
  "include": ["src"]
}
